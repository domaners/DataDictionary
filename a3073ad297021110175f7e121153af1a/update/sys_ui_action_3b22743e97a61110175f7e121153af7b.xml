        <name>Confirm and close update set</name>
        <onclick/>
        <order>100</order>
        <script><![CDATA[confirmAndClose();

function confirmAndClose() {
    var impactedFields = new GlideAggregate("x_78983_data_dicti_impacted_field");
    impactedFields.addQuery("dictionary_update", current.sys_id + '');
    impactedFields.addNullQuery("impact_status");
    impactedFields.addAggregate("COUNT");
    impactedFields.query();
    impactedFields.next();
    if (impactedFields.getAggregate("COUNT") > 0) {
        gs.addInfoMessage("Please set the Impact status value on all Impacted fields before attempting to close.");
    } else {
        current.state = 3;
		gs.eventQueue("x_78983_data_dicti.close_source_update_s", current); // Call API to complete update set on source instance  
	}
    current.update();
    action.setRedirectURL(current);
}]]></script>
